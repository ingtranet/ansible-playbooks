--tserver_master_addrs=yugabyte-m-1.mdc.ingtra.net:7100,yugabyte-m-2.mdc.ingtra.net:7100,yugabyte-m-3.mdc.ingtra.net:7100
--placement_cloud=ingtranet
--placement_region=mdc
--placement_zone=default
--fs_wal_dirs=/var/lib/yugabyte
--fs_data_dirs=/data/yugabyte
--enable_stream_compression=true
--stream_compression_algo=3
--enable_ondisk_compression=true
--compression_type=LZ4
--server_broadcast_addresses={{ ansible_hostname }}.mdc.ingtra.net
--yb_num_shards_per_tserver=8
--ysql_num_shards_per_tserver=8
--default_memory_limit_to_ram_ratio=0.3
--durable_wal_write=false
--pg_yb_session_timeout_ms=600000